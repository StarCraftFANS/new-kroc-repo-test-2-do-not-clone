#
# OS X
#
plugin.OccPlugPlugin.path.kroc.osx.include=share/kroc/vtinclude
plugin.OccPlugPlugin.path.kroc.osx.lib=share/kroc/vtlib
plugin.OccPlugPlugin.path.kroc.osx.bin=bin

plugin.OccPlugPlugin.path.tvm.osx.include=share/tvm/vtinclude
plugin.OccPlugPlugin.path.tvm.osx.lib=share/tvm/vtlib
plugin.OccPlugPlugin.path.tvm.osx.firmware=share/tvm/firmware
plugin.OccPlugPlugin.path.tvm.osx.bin=bin
plugin.OccPlugPlugin.path.tvm.osx-ppc.include=share/tvm-ppc/vtinclude
plugin.OccPlugPlugin.path.tvm.osx-ppc.lib=share/tvm-ppc/vtlib
plugin.OccPlugPlugin.path.tvm.osx-ppc.firmware=share/tvm-ppc/firmware

plugin.OccPlugPlugin.path.tvm-arduino.osx.conf=
plugin.OccPlugPlugin.path.tvm-arduino.osx.firmware=
plugin.OccPlugPlugin.path.tvm-arduino.osx.include=
plugin.OccPlugPlugin.path.tvm-arduino.osx.lib=
plugin.OccPlugPlugin.path.tvm-arduino.osx.bin=bin
plugin.OccPlugPlugin.path.tvm-arduino.osx.etc=bin

#
# Windows
#
plugin.OccPlugPlugin.path.tvm.windows.include=posix/tvm/include
plugin.OccPlugPlugin.path.tvm.windows.lib=posix/tvm/lib
plugin.OccPlugPlugin.path.tvm.windows.firmware=posix/tvm/firmware

plugin.OccPlugPlugin.path.tvm-arduino.windows.conf=arduino/tvm/conf
plugin.OccPlugPlugin.path.tvm-arduino.windows.firmware=arduino/tvm/firmware
plugin.OccPlugPlugin.path.tvm-arduino.windows.include=arduino/tvm/include
plugin.OccPlugPlugin.path.tvm-arduino.windows.lib=arduino/tvm/lib
plugin.OccPlugPlugin.path.tvm-arduino.windows.bin=bin
plugin.OccPlugPlugin.path.tvm-arduino.windows.etc=bin

#
# Ubuntu
# 
plugin.OccPlugPlugin.path.kroc.ubuntu.include=/usr/share/kroc
plugin.OccPlugPlugin.path.kroc.ubuntu.lib=/usr/share/kroc/lib
plugin.OccPlugPlugin.path.tvm.ubuntu.include=/usr/share/tvm/include
plugin.OccPlugPlugin.path.tvm.ubuntu.lib=/usr/share/tvm/lib
plugin.OccPlugPlugin.path.tvm.ubuntu.firmware=/usr/share/tvm/firmware
plugin.OccPlugPlugin.path.tvm.ubuntu.bin=/usr/bin
plugin.OccPlugPlugin.path.kroc.ubuntu.bin=/usr/bin

plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.conf=/usr/share/tvm-avr/conf
plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.firmware=/usr/share/tvm-avr/firmware
plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.include=/usr/share/tvm-avr/include
# There is no libdir for AVR on Ubuntu at this time.
plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.lib=/usr/lib
plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.bin=/usr/bin
# Use the conf from the avrdude package. The .conf lives in etc.
plugin.OccPlugPlugin.path.tvm-arduino.ubuntu.etc=/etc
